    "vue.js",
    "parcel",
    "es6",
    "ssr"
  ],
  "author": "Anna Novikova (anna@novikova.us)",
  "license": "MIT",
  "devDependencies": {
    "fastify": "^2.3.0",
    "mongodb": "^3.2.3",
    "parcel-bundler": "^1.12.3"
  }
}
